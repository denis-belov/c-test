XGK=E:/reps/denis-belov/xgk_/src
DIR=E:/reps/denis-belov/c-test/xgk
NAME=threads



$(DIR)/build-debug/$(NAME).exe : \
$(DIR)/tmp/glad.obj \
$(DIR)/tmp/win-x64.obj \
$(DIR)/tmp/vec4.obj \
$(DIR)/tmp/vec4-32.obj \
$(DIR)/tmp/vec4-128.obj \
$(DIR)/tmp/quat-32.obj \
$(DIR)/tmp/quat-128.obj \
$(DIR)/tmp/$(NAME).obj
  link \
  $(DIR)/tmp/glad.obj \
  $(DIR)/tmp/win-x64.obj \
  $(DIR)/tmp/vec4.obj \
  $(DIR)/tmp/vec4-32.obj \
  $(DIR)/tmp/vec4-128.obj \
  $(DIR)/tmp/quat-32.obj \
  $(DIR)/tmp/quat-128.obj \
  $(DIR)/tmp/$(NAME).obj \
  gdi32.lib \
  user32.lib \
  shell32.lib \
  glfw3.lib \
  /OUT:$(DIR)/build-debug/$(NAME).exe \
  /SUBSYSTEM:CONSOLE \
  /NODEFAULTLIB:LIBCMT \
  /LIBPATH:E:/clibs/glfw-3.3.bin.WIN64/glfw-3.3.bin.WIN64/lib-vc2017


$(DIR)/tmp/glad.obj : E:/clibs/glad/src/glad.c
  cl \
  /c \
  /EHsc \
  /MD \
  /MP999 \
  /IE:/clibs/glad/include \
  /Fo$(DIR)/tmp/glad.obj \
  E:/clibs/glad/src/glad.c



$(DIR)/tmp/win-x64.obj : $(XGK)/util/win-x64.asm
  ml64 \
  /Fo$(DIR)/tmp/win-x64.obj \
  /c \
  $(XGK)/util/win-x64.asm



$(DIR)/tmp/vec4.obj : $(XGK)/data/vec4/vec4.cpp
